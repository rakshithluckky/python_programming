#Python program to add three given lists using lambda and map
l1=[1,2,3]
l2=[23,23,23]
l3=[4,63,12]
print("original lists:")
print(l1,"\n",l2,"\n",l3,end="\n")
result=map(lambda x,y,z:x+y+z,l1,l2,l3)
print("new list after adding above three lists")
print(list(result))
#program for adding all the elements in the lists
l=l1+l2+l3
print(l)
l=list(map(str,l))
print(l)
s="+"
x=s.join(l)
print(eval(x))
